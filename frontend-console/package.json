    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/typed-document-node": "^2.2.5",
        "@graphql-codegen/typescript": "2.4.5",
        "@graphql-codegen/typescript-operations": "^2.3.2",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@types/node": "^17.0.17",
        "@types/yargs": "^17.0.8",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@cartesi/rollups": "^0.5.0",
        "@ethersproject/providers": "^5.5.3",
        "@urql/core": "^2.4.3",
        "cross-fetch": "^3.1.5",
        "ethers": "^5.5.4",
        "graphql": "^16.3.0",
        "yargs": "^17.3.1"
    }
