{
    "name": "frontend-console",
    "version": "0.8.0",
    "description": "Simple console front-end for Cartesi DApp",
    "author": "Danilo Tuler <danilo.tuler@cartesi.io>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "run-s codegen tsc",
        "codegen": "graphql-codegen",
        "clean": "rimraf dist",
        "prepack": "yarn build",
        "prettier": "prettier -w \"**/*.ts\"",
